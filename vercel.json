{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/ai-builder/full-app/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/ai-builder/full-app-stream/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/ai-builder/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/ai-builder/modify/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/ai-builder/plan-phases/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/chat/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/wizard/chat/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/wizard/generate-phases/route.ts": {
      "maxDuration": 120
    }
  }
}
